{
    "name": "urbanindo/yii2-dynamodb",
    "description": "DynamoDB extensions for Yii2",
    "license": "BSD License",
    "authors": [
        {
            "name": "Petra Barus",
            "email": "petra.barus@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.9,<2.1",
        "aws/aws-sdk-php": "^3.28"
    },
    "require-dev": {
