        "phpunit/php-code-coverage": "2.2.4",
        "fzaninotto/faker": "dev-master",
        "flow/jsonpath": "dev-master",
        "yiisoft/yii2-coding-standards": "*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "UrbanIndo\\Yii2\\DynamoDb\\": "src/"
        }
    }
